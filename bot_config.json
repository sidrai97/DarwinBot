{ 
  "get_started":{
    "payload":"get_started"
  },
  "greeting":[
	{
    	"locale":"default",
    	"text":"Hello {{user_first_name}}! Ready to appoint me as your health assistant?"
	}
  ],
  "persistent_menu":[
	{
    	"locale":"default",
    	"composer_input_disabled": false,
    	"call_to_actions":[
    		{
        		"title":"Features Menu",
        		"type":"nested",
        		"call_to_actions":[
        			{
            			"title":"Symptom Checker",
            			"type":"postback",
            			"payload":"symptom_checker"
        			},
        			{
            			"title":"Health Tracker",
            			"type":"nested",
            			"call_to_actions":[
            				{
            					"title":"Plan my workout",
            					"type":"postback",
            					"payload":"plan_my_workout"
            				},
            				{
            					"title":"Get Workout Recommendations",
            					"type":"postback",
            					"payload":"get_workout_recommendations"
            				},
            				{
            					"title":"My Workout Statistics",
            					"type":"postback",
            					"payload":"my_workout_statistics"
            				}
            			]
        			},
        			{
            			"title":"Exercises Guide",
            			"type":"postback",
            			"payload":"exercises_guide"
        			}
        		]
    		}
        ]
    }
  	]
}